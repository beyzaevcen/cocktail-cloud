{
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "/cocktail/:id",
        "destination": "/index.html"
      },
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ],
    "redirects": [
      {
        "source": "/cocktail/:id",
        "destination": "/index.html",
        "permanent": false
      }
    ]
  }
  